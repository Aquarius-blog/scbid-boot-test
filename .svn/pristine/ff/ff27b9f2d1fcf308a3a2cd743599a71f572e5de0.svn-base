
<!-- 模态框（Modal） -->
<div class="modal fade" id="supplierNoticeModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
	<div class="modal-dialog">
		<div class="modal-content">
			<div class="modal-body">
        	<form class="form-horizontal">
				
            	<div class="form-group">
                 <p>请阅读供应商须知</p>
              	</div>
              	<input id="notice" type="radio"  @click="select" >
              </form>
    		</div>
			<div class="modal-footer">
				<button type="button" class="btn btn-default" @click="sure">确定</button>
				<button type="button" class="btn btn-default" data-dismiss="modal">关闭 </button>
			</div>
		</div><!-- /.modal-content -->
	</div><!-- /.modal-dialog -->
</div><!-- /.modal -->